#! /usr/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 10;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x5653e8e87100 .scope module, "main_math" "main_math" 2 3;
 .timescale -9 -10;
P_0x5653e8e87280 .param/l "SYSCLK_PERIOD" 0 2 7, +C4<00000000000000000000000000001010>;
v0x5653e8ecf810_0 .var "SYSCLK", 0 0;
v0x5653e8ecf8d0_0 .var "ai", 0 0;
v0x5653e8ecf9a0_0 .var "bi", 0 0;
v0x5653e8ecfaa0_0 .var "counter", 0 0;
v0x5653e8ecfb40_0 .net "sum", 0 0, v0x5653e8ecf6a0_0;  1 drivers
E_0x5653e8e8e140 .event edge, v0x5653e8ecf5d0_0;
S_0x5653e8e862f0 .scope module, "lib_math_ut0" "lib_math" 2 26, 3 2 0, S_0x5653e8e87100;
 .timescale -9 -10;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 1 "a"
    .port_info 2 /INPUT 1 "b"
    .port_info 3 /OUTPUT 1 "sum"
v0x5653e8e86510_0 .net "a", 0 0, v0x5653e8ecf8d0_0;  1 drivers
v0x5653e8ecf510_0 .net "b", 0 0, v0x5653e8ecf9a0_0;  1 drivers
v0x5653e8ecf5d0_0 .net "clk", 0 0, v0x5653e8ecf810_0;  1 drivers
v0x5653e8ecf6a0_0 .var "sum", 0 0;
E_0x5653e8ec0080 .event posedge, v0x5653e8ecf5d0_0;
    .scope S_0x5653e8e862f0;
T_0 ;
    %wait E_0x5653e8ec0080;
    %load/vec4 v0x5653e8ecf5d0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %load/vec4 v0x5653e8ecf6a0_0;
    %load/vec4 v0x5653e8e86510_0;
    %add;
    %assign/vec4 v0x5653e8ecf6a0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5653e8ecf6a0_0;
    %load/vec4 v0x5653e8ecf510_0;
    %sub;
    %assign/vec4 v0x5653e8ecf6a0_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5653e8e87100;
T_1 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5653e8ecfaa0_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x5653e8e87100;
T_2 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5653e8ecf9a0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5653e8ecf8d0_0, 0, 1;
T_2.0 ;
    %load/vec4 v0x5653e8ecfaa0_0;
    %pad/u 32;
    %cmpi/u 10, 0, 32;
    %flag_or 5, 4;
    %jmp/0xz T_2.1, 5;
    %load/vec4 v0x5653e8ecf8d0_0;
    %pad/u 2;
    %addi 1, 0, 2;
    %pad/u 1;
    %store/vec4 v0x5653e8ecf8d0_0, 0, 1;
    %load/vec4 v0x5653e8ecf9a0_0;
    %pad/u 3;
    %addi 2, 0, 3;
    %pad/u 1;
    %store/vec4 v0x5653e8ecf9a0_0, 0, 1;
    %load/vec4 v0x5653e8ecfaa0_0;
    %pad/u 2;
    %addi 1, 0, 2;
    %pad/u 1;
    %store/vec4 v0x5653e8ecfaa0_0, 0, 1;
    %jmp T_2.0;
T_2.1 ;
    %delay 50, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5653e8ecf810_0, 0, 1;
    %delay 10000, 0;
    %vpi_call 2 20 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x5653e8e87100;
T_3 ;
    %wait E_0x5653e8e8e140;
    %delay 50, 0;
    %load/vec4 v0x5653e8ecf810_0;
    %nor/r;
    %assign/vec4 v0x5653e8ecf810_0, 0;
    %jmp T_3;
    .thread T_3, $push;
    .scope S_0x5653e8e87100;
T_4 ;
    %vpi_call 2 39 "$dumpfile", "wave.vcd" {0 0 0};
    %vpi_call 2 40 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5653e8e87100 {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "main_math.v";
    ".//lib_math.v";
